{
  "name": "jupiter-perps-anchor-idl-parsing",
  "version": "0.0.1",
  "description": "A sample project to work with the Jupiter Perps IDL with Anchor",
  "main": "dist/index.js",
  "scripts": {
    "script": "ts-node-dev --respawn --transpile-only",
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "docker:build": "docker build -t jupiter-perps-jlp-api .",
    "docker:run": "docker run -p 3000:3000 jupiter-perps-jlp-api"
  },
  "keywords": [
    "typescript",
    "node"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.95.3",
    "cors": "^2.8.5",
    "decimal.js": "^10.6.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/node": "^20.10.5",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
